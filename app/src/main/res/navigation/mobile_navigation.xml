<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.healthkeeper_new.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_trainerbot"
        android:name="com.example.healthkeeper_new.ui.trainerbot.TrainerbotFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_trainerbot" />

    <fragment
        android:id="@+id/navigation_inbody"
        android:name="com.example.healthkeeper_new.ui.inbody.InbodyFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_inbody" />
</navigation>
